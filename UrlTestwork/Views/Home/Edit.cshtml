@model UrlTestwork.Models.CuttLink
@{
    ViewBag.Title = "Редактирование сокращенной URL";
}
<form asp-action="edit" asp-controller="home" asp-route-id="@Model.Id">
    <div class="form-group">
        <label class="control-label">Ваш URL</label>
        <input type="text" asp-for="Url" class="form-control" />
    </div>
    <div class="form-group">
        <input type="submit" value="Изменить" class="btn btn-primary" />
    </div>
</form>